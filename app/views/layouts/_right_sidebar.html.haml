  %ul#categories
    - @categories.each do |category|
      = render :partial => 'categories/category', :object => category

  = semantic_form_for @report, :html => {:multipart => true }, :format => :html, :url => send_report_api_reports_path do |f|
    = f.input :title, :input_html => {:value => "asdasdasd123123"}
